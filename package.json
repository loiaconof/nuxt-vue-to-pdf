{
  "name": "nuxt-vue-to-pdf",
  "version": "1.0.2",
  "description": "My new Nuxt module",
  "repository": "loiaconof/nuxt-vue-to-pdf",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "release": "npm run lint && npm run test && npm run prepack && changelogen --release && npm publish && git push --follow-tags",
    "lint": "eslint .",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:types": "vue-tsc --noEmit && cd playground && vue-tsc --noEmit",
    "docker:build": "docker build . -t nuxt-vue-to-pdf",
    "docker:run": "docker run --rm -p 8082:80 -t nuxt-vue-to-pdf:latest",
    "docker:run:it": "docker run --rm -p 8082:80 -it -t nuxt-vue-to-pdf:latest /bin/sh",
    "docker:up": "npm run docker:build && npm run docker:run"
  },
  "dependencies": {
    "@nuxt/kit": "^3.14.0",
    "@vitejs/plugin-vue": "^5.1.4",
    "defu": "^6.1.4",
    "postcss": "^8.4.47",
    "postcss-import": "^16.1.0",
    "puppeteer": "^23.7.0",
    "rollup-plugin-postcss": "^4.0.2"
  },
  "devDependencies": {
    "@nuxt/devtools": "^1.6.0",
    "@nuxt/eslint-config": "^0.5.7",
    "@nuxt/module-builder": "^0.8.4",
    "@nuxt/schema": "^3.14.0",
    "@nuxt/test-utils": "^3.14.4",
    "@types/node": "latest",
    "changelogen": "^0.5.7",
    "eslint": "^9.14.0",
    "nuxt": "^3.14.0",
    "typescript": "latest",
    "vitest": "^2.1.4",
    "vue-tsc": "^2.1.10"
  },
  "packageManager": "pnpm@9.4.0+sha512.f549b8a52c9d2b8536762f99c0722205efc5af913e77835dbccc3b0b0b2ca9e7dc8022b78062c17291c48e88749c70ce88eb5a74f1fa8c4bf5e18bb46c8bd83a"
}
